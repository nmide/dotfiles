-[3.2]-+ */\n/* +------------------------------------------------------------------------[3]-+ */\n/* +----------------------------------------------------------------------------+\n   + Font                                                                 [4] +\n   +----------------------------------------------------------------------------+ */\n/* +-----------------------------------------------------------------------[4]-+ */\n.settings-view {\n  display: flex;\n}\n.settings-view .breadcrumb {\n  margin-bottom: 0;\n  padding: 3em;\n  font-size: 1.2em;\n  color: rgba(69, 69, 70, 0.6);\n  list-style: none;\n}\n.settings-view .breadcrumb > li {\n  display: inline-block;\n}\n.settings-view .breadcrumb > li a {\n  color: #454546;\n}\n.settings-view .breadcrumb > li + li:before {\n  content: \"/\\00a0\";\n  padding: 0 5px;\n}\n.settings-view .breadcrumb > .active a {\n  color: #404041;\n}\n.settings-view .text {\n  color: #454546;\n}\n.settings-view .alert-info {\n  color: #3a759b;\n  border-color: #c1d0e3;\n  background-color: #e5ebf3;\n}\n.settings-view .alert-info .alert-link {\n  color: inherit;\n  text-decoration: underline;\n}\n.settings-view .alert-success {\n  color: #8e9232;\n  border-color: #d9dac9;\n  background-color: #eeefe7;\n}\n.settings-view .alert-success .alert-link {\n  color: inherit;\n  text-decoration: underline;\n}\n.settings-view .alert-warning {\n  color: #ff9e02;\n  border-color: #e2d3c7;\n  background-color: #f3eee9;\n}\n.settings-view .alert-warning .alert-link {\n  color: inherit;\n  text-decoration: underline;\n}\n.settings-view .alert-error,\n.settings-view .alert-danger {\n  color: #b12422;\n  border-color: #e0c0ca;\n  background-color: #f1e2e7;\n}\n.settings-view .alert-error .alert-link,\n.settings-view .alert-danger .alert-link {\n  color: inherit;\n  text-decoration: underline;\n}\n.settings-view .error-message .alert {\n  margin-bottom: 0;\n}\n.settings-view .error-message .close {\n  top: -6px;\n}\n.settings-view .error-message .error-details {\n  color: #b12422;\n  border-color: #e0c0ca;\n  background-color: #f1e2e7;\n}\n.settings-view .error-message .error-details .alert-link {\n  color: inherit;\n  text-decoration: underline;\n}\n.settings-view .error-message .error-link {\n  color: inherit;\n  text-decoration: underline;\n  margin-left: 0.75em;\n}\n.settings-view select.form-control {\n  color: #454546;\n  border-color: #f9f9fc;\n  background-color: #71a7ca;\n}\n.settings-view select.form-control:hover,\n.settings-view select.form-control:focus {\n  box-shadow: none;\n  background-color: #78accd;\n}\n.settings-view .select-container {\n  display: flex;\n  align-items: stretch;\n}\n.settings-view .btn.active-theme-settings {\n  margin-left: 2px;\n}\n.settings-view .btn.active-theme-settings::before {\n  margin-right: 0;\n  text-align: left;\n}\n.settings-view .checkbox {\n  padding-left: 2.25em;\n}\n.settings-view .checkbox label {\n  padding-left: 0;\n  display: block;\n}\n.settings-view .checkbox .setting-title {\n  display: inline-block;\n}\n.settings-view input[type=\"checkbox\"] {\n  -webkit-appearance: none;\n  display: inline-block;\n  position: relative;\n  font-size: inherit;\n  margin: 0 0.75em 0 -2.25em;\n  vertical-align: top;\n  width: 1.5em;\n  height: 1.5em;\n  cursor: pointer;\n  outline: 0;\n  border-radius: 2px;\n  background-color: rgba(69, 69, 70, 0.6);\n  transition: background-color 0.16s cubic-bezier(0.5, 0.15, 0.2, 1);\n}\n.settings-view input[type=\"checkbox\"]:focus {\n  box-shadow: 0 0 0 1px #f9f9fc;\n}\n.settings-view input[type=\"checkbox\"]:active {\n  background-color: #cbcf5b;\n}\n.settings-view input[type=\"checkbox\"]:before,\n.settings-view input[type=\"checkbox\"]:after {\n  content: \"\";\n  position: absolute;\n  top: 1.1em;\n  left: .6em;\n  height: .16em;\n  min-height: 2px;\n  border-radius: 1px;\n  background-color: #f2f2f7;\n  transform-origin: 0% 0%;\n  opacity: 0;\n  transition: transform 0.1s cubic-bezier(0.5, 0.15, 0.2, 1), opacity 0.1s cubic-